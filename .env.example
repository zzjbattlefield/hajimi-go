# GitHub API Tokens (comma-separated)
GITHUB_TOKENS=ghp_your_token_here_1,ghp_your_token_here_2

# Proxy server address (optional)
# PROXY=http://user:pass@proxy:port

# Data storage directory path
DATA_PATH=./data
SAVE_FILE_NAME=valid_keys.txt
# Repository age filter (days)
DATE_RANGE_DAYS=730

# Search queries file path
QUERIES_FILE=queries.txt

# Sync settings
SYNC_ENABLED=false
SYNC_BATCH_SIZE=10
SYNC_FLUSH_INTERVAL=30
SYNC_MAX_RETRIES=3